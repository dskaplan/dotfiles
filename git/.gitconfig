# This is Git's per-user configuration file.
[user]
	# name = David
  # email = david@dkpln.com
[color]
        ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = red
[alias]
        br = branch -av
        a = add --all
        cm = commit -m
        d = diff
        f = fetch
        s = status
        sb = status -s -b
[core]
  # excludesfile = /Users/DKaplan/.gitignore_global
